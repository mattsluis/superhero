<div class="col-sm-12">
  <h4 class="results"> Results:</h4>
    <div class="col-xs-6 col-s-6 col-m-6 col-l-6 col-xl-6">
      <form id="formLeft" class="navbar-form" role="search">
        <input id="inputLeft" type="text" class="form-control" id="inputLeft" placeholder="Search" name="q"></input>
        <button id="buttonLeft" class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
      </form>

        <% heroes.forEach(function(hero) { %>
        <h5 id="name"><%= hero.name %></h5>
        <img id="imgLink" src="<%= hero.thumbnail.path + '/portrait_medium' + '.' + hero.thumbnail.extension %>" class="heroImg"/>
        <p id="description">DESCRIPTION: <%= hero.description %></p>
        <li id="storyCount">STORIES AVAILABLE: <%= hero.stories.available %></li>

      <% }); %>
    </div>


    <div class="col-xs-6 col-s-6 col-m-6 col-l-6 col-xl-6">
      <form id="formRight" class="navbar-form" role="search">
        <input id="inputRight" type="text" class="form-control" placeholder="Search" name="q"></input>
        <button id="buttonRight" class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
      </form>
      <% heroes.forEach(function(hero) { %>
      <h5><%= hero.name %></h5>
      <img src="<%= hero.thumbnail.path + '/portrait_medium' + '.' + hero.thumbnail.extension %>" class="heroImg"/>
      <p>DESCRIPTION: <%= hero.description %></p>
      <li>STORIES AVAILABLE: <%= hero.stories.available %></li>
       <%= hero.stories.items %>

      <% }); %>
    </div>


</div>
